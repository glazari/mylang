fn is_prime(num) {
  let i = 2;
  while (i < num) {
    if (num % i == 0) {
      return 0;
    }
    i = i + 1;
  }
  return 1;
}

fn print_numberln(num) {
  let num_size = 0;
  let cp_num = num;
  while (cp_num > 0) {
    cp_num = cp_num / 10;
    num_size = num_size + 1;
  }
  let digit = 0;
  let base = 0;

  while (num > 0) {
    base = pow(10, num_size-1);
    digit = num / base + 48;
    print_chr(digit);
    num = num % base;
    num_size = num_size - 1;
  } 
  print_chr(10);
}

fn pow(base, exp) {
  let prod = 1;
  while (exp > 0) {
    prod = prod * base;
    exp = exp - 1;
  }
  return prod;
}

fn print_chr(a) {
  asm {
    mov rsi, rbp
    add rsi, 24 ; point to address of a
    mov rdx, 1 ; length
    mov rax, 1 ; write syscall
    mov rdi, 1 ; stdout file handle
    syscall
  }
}
